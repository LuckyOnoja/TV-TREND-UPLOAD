{
  "version": 2,
  "builds": [
    {
      "src": "server/*",
      "use": "@vercel/node"
    },
    {
      "src": "build/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/pages/.*",
      "dest": "server/pages/home"
    },
    {
      "src": "/(.*)",
      "dest": "build/$1"
    }
  ]
}
